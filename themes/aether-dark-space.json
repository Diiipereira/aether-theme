  "tokenColors": [
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "variable.language"
      ],
      "settings": {
        "foreground": "#bd5eff"
      }
    },
    {
      "name": "Decorators & Annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "entity.name.function.decorator",
        "meta.decorator variable.other.readwrite",
        "meta.decorator entity.name.function"
      ],
      "settings": {
        "foreground": "#ffcb6b"
      }
    },
    {
      "name": "Control Flow",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#bd5eff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#c9b8db"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#ffcb6b"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": [
        "variable",
        "support.variable",
        "meta.object-literal.key",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#4fffd0"
      }
    },
    {
      "name": "Classes & Structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "storage.type.cs"
      ],
      "settings": {
        "foreground": "#5ee6ff"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": [
        "entity.name.type.interface",
        "support.type",
        "entity.name.type.ts"
      ],
      "settings": {
        "foreground": "#5ee6ff"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#ff9ccc"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#7acfff"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#ff70d9"
      }
    },
    {
      "name": "Comments Base",
      "scope": "comment",
      "settings": {
        "foreground": "#a69eb0"
      }
    },
    {
      "name": "Comments Italic",
      "scope": [
        "comment.line",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Imports & Exports",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "meta.import string"
      ],
      "settings": {
        "foreground": "#bd5eff"
      }
    },
    {
      "name": "JS/TS this/super",
      "scope": [
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#ff70d9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Console/DOM",
      "scope": [
        "support.type.object.console",
        "support.type.object.dom"
      ],
      "settings": {
        "foreground": "#5ee6ff"
      }
    },
    {
      "name": "JSX/TSX Component Tags",
