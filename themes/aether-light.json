  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#9ca3af",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#16a34a"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#7c3aed"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.operator.accessor"
      ],
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "scope": [
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#7c3aed",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "meta.property.object",
        "support.variable.property",
        "meta.object.member"
      ],
      "settings": {
        "foreground": "#0e7490"
      }
    },
    {
      "scope": [
        "variable.other.readwrite",
        "meta.objectliteral variable.other.property"
      ],
      "settings": {
        "foreground": "#1f2937"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "meta.function.parameters variable.parameter"
      ],
      "settings": {
        "foreground": "#0891b2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#1f2937"
      }
    },
    {
      "scope": [
        "constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#d97706"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#f97316"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.other.inherited-class",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#0e7490"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#6b7280"
      }
    },
    {
      "scope": [
        "meta.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#9333ea"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "entity.name.tag.html",
        "meta.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#1d4ed8"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#6b7280"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#7c3aed"
      }
    },
    {
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted",
        "string.html"
      ],
      "settings": {
        "foreground": "#16a34a"
      }
    },
    {
      "scope": [
        "meta.tag.sgml.doctype",
        "keyword.other.doctype"
      ],
      "settings": {
        "foreground": "#6b7280"
      }
    },
    {
      "scope": [
        "comment.block.html",
        "punctuation.definition.comment.html"
      ],
      "settings": {
        "foreground": "#9ca3af"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#7c3aed",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.quote",
        "punctuation.definition.blockquote"
      ],
      "settings": {
        "foreground": "#6b7280",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.underline.link",
        "markup.underline"
      ],
      "settings": {
        "foreground": "#2563eb",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "string.other.link.title",
        "markup.link"
      ],
      "settings": {
        "foreground": "#16a34a"
      }
    },
    {
      "scope": [
        "markup.list",
        "punctuation.definition.list.begin"
      ],
      "settings": {
        "foreground": "#1f2937"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.raw.inline"
      ],
      "settings": {
        "foreground": "#16a34a",
        "background": "#e5e7eb"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#1f2937",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#1f2937",
        "fontStyle": "italic"
      }
    }
  ]
}