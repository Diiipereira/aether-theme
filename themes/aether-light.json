  "tokenColors": [
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "variable.language"
      ],
      "settings": {
        "foreground": "#9c6b94"
      }
    },
    {
      "name": "Decorators & Annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "entity.name.function.decorator",
        "meta.decorator variable.other.readwrite",
        "meta.decorator entity.name.function"
      ],
      "settings": {
        "foreground": "#5a7699"
      }
    },
    {
      "name": "Control Flow",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#9c6b94",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#8f857d"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#5a7699"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": [
        "variable",
        "support.variable",
        "meta.object-literal.key",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#4f4742"
      }
    },
    {
      "name": "Classes & Structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "storage.type.cs"
      ],
      "settings": {
        "foreground": "#508a8a"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": [
        "entity.name.type.interface",
        "support.type",
        "entity.name.type.ts"
      ],
      "settings": {
        "foreground": "#508a8a"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#5c8c63"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b07d62"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#6e7996"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#a69b91",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Imports & Exports",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "meta.import string"
      ],
      "settings": {
        "foreground": "#9c6b94"
      }
    },
    {
      "name": "JS/TS this/super",
