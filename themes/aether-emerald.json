      }
    },
    {
      "name": "Constants, Booleans, Null",
      "scope": "constant.language, constant.other",
      "settings": {
        "foreground": "#22d3ee"
      }
    },
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#5c756a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Import/Export",
