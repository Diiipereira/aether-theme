  "tokenColors": [
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "variable.language"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Decorators & Annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "entity.name.function.decorator",
        "meta.decorator variable.other.readwrite",
        "meta.decorator entity.name.function"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Control Flow",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#34d399",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#94a3b8"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": [
        "variable",
        "support.variable",
        "meta.object-literal.key",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#e2e8f0"
      }
    },
    {
      "name": "Classes & Structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "storage.type.cs"
      ],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": [
        "entity.name.type.interface",
        "support.type",
        "entity.name.type.ts"
      ],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#a7f3d0"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#c084fc"
      }
    },
    {
      "name": "Comments Base",
      "scope": "comment",
      "settings": {
        "foreground": "#829c95"
      }
    },
    {
      "name": "Comments Italic",
      "scope": [
        "comment.line",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Imports & Exports",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "meta.import string"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "JS/TS this/super",
