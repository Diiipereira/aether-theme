  "tokenColors": [
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "variable.language"
      ],
      "settings": {
        "foreground": "#e6b88a"
      }
    },
    {
      "name": "Decorators & Annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "entity.name.function.decorator",
        "meta.decorator variable.other.readwrite",
        "meta.decorator entity.name.function"
      ],
      "settings": {
        "foreground": "#a7c080"
      }
    },
    {
      "name": "Control Flow",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#e6b88a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#7fbbb3"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#a7c080"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": [
        "variable",
        "support.variable",
        "meta.object-literal.key",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#e8dccf"
      }
    },
    {
      "name": "Classes & Structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "storage.type.cs"
      ],
      "settings": {
        "foreground": "#8ab4c9"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": [
        "entity.name.type.interface",
        "support.type",
        "entity.name.type.ts"
      ],
      "settings": {
        "foreground": "#8ab4c9"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#dbbc7f"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#d699b6"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#8ab4c9"
      }
    },
    {
      "name": "Comments Base",
      "scope": "comment",
      "settings": {
        "foreground": "#998b82"
      }
    },
    {
      "name": "Comments Italic",
      "scope": [
        "comment.line",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Imports & Exports",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "meta.import string"
      ],
      "settings": {
        "foreground": "#e6b88a"
      }
    },
    {
      "name": "JS/TS this/super",
      "scope": [
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#8ab4c9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Console/DOM",
      "scope": [
        "support.type.object.console",
        "support.type.object.dom"
      ],
      "settings": {
        "foreground": "#8ab4c9"
      }
    },
    {
