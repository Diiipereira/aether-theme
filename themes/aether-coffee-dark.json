  "tokenColors": [
    {
      "name": "Keywords (if, else, return, public)",
      "scope": "keyword, storage.type, storage.modifier",
      "settings": {
        "foreground": "#d17b49"
      }
    },
    {
      "name": "Control Flow (try, catch, loop)",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#d17b49",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators (=, +, -, >, ?)",
      "scope": "keyword.operator, punctuation.separator, punctuation.terminator",
      "settings": {
        "foreground": "#7aa0b9"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#e5ad4c"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": "variable, support.variable, meta.object-literal.key",
      "settings": {
        "foreground": "#c29d7e"
      }
    },
    {
      "name": "Classes",
      "scope": "entity.name.type, entity.name.class, support.class",
      "settings": {
        "foreground": "#909973"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": "entity.name.type.interface, support.type",
      "settings": {
        "foreground": "#909973"
      }
    },
    {
      "name": "Strings",
      "scope": "string, constant.character.escape",
      "settings": {
        "foreground": "#d17b49"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#e5ad4c"
      }
    },
    {
      "name": "Constants, Booleans, Null",
      "scope": "constant.language, constant.other",
      "settings": {
        "foreground": "#909973"
      }
    },
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#8a7968",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Import/Export",
      "scope": "keyword.control.import, keyword.control.export, keyword.control.from",
      "settings": {
        "foreground": "#d17b49"
      }
    },
    {
      "name": "JS/TS `this`, `super`",
      "scope": "variable.language.this, variable.language.super",
      "settings": {
        "foreground": "#909973",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Console",
