  "tokenColors": [
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "variable.language"
      ],
      "settings": {
        "foreground": "#d8966c"
      }
    },
    {
      "name": "Decorators & Annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "entity.name.function.decorator",
        "meta.decorator variable.other.readwrite",
        "meta.decorator entity.name.function"
      ],
      "settings": {
        "foreground": "#e0c080"
      }
    },
    {
      "name": "Control Flow",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#d8966c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#9e9285"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#e0c080"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": [
        "variable",
        "support.variable",
        "meta.object-literal.key",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#e6dccb"
      }
    },
    {
      "name": "Classes & Structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "storage.type.cs"
      ],
      "settings": {
        "foreground": "#d4a373"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": [
        "entity.name.type.interface",
        "support.type",
        "entity.name.type.ts"
      ],
      "settings": {
        "foreground": "#d4a373"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#a7b886"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b095ad"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#d4a373"
      }
    },
    {
      "name": "Comments Base",
      "scope": "comment",
      "settings": {
        "foreground": "#9e9285"
      }
    },
    {
      "name": "Comments Italic",
      "scope": [
        "comment.line",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Imports & Exports",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "meta.import string"
      ],
      "settings": {
        "foreground": "#d8966c"
      }
    },
    {
      "name": "JS/TS this/super",
      "scope": [
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#d4a373",
