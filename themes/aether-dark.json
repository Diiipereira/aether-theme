  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#565f89"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#9ece6a"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.operator.accessor"
      ],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "name": "This Keyword",
      "scope": [
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#d7bfff"
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "meta.property.object",
        "support.variable.property",
        "meta.object.member"
      ],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "Local Variables",
      "scope": [
        "variable.other.readwrite",
        "meta.objectliteral variable.other.property"
      ],
      "settings": {
        "foreground": "#c0caf5"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter",
        "meta.function.parameters variable.parameter"
      ],
      "settings": {
        "foreground": "#5ec4e8"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#e5e7eb"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#e0af68"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Types / Interfaces",
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.other.inherited-class",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#7bb0ff"
      }
    },
    {
      "name": "Operators / Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#c0caf5"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#c586e9"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.html",
        "meta.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "HTML Tag Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#c0caf5"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#d7bfff"
      }
    },
    {
      "name": "HTML Values",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted",
        "string.html"
      ],
      "settings": {
        "foreground": "#9ece6a"
      }
    },
    {
      "name": "HTML Doctype",
      "scope": [
        "meta.tag.sgml.doctype",
        "keyword.other.doctype"
      ],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "HTML Comments",
      "scope": [
        "comment.block.html",
        "punctuation.definition.comment.html"
      ],
      "settings": {
        "foreground": "#565f89"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "markup.heading entity.name",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#bb9af7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Quotes",
      "scope": [
        "markup.quote",
        "punctuation.definition.blockquote"
      ],
      "settings": {
        "foreground": "#a9a1d6"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "markup.underline"
      ],
      "settings": {
        "foreground": "#7aa2f7",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title",
        "markup.link"
      ],
      "settings": {
        "foreground": "#9ece6a"
      }
    },
    {
      "name": "Markdown Lists",
      "scope": [
        "markup.list",
        "punctuation.definition.list.begin"
      ],
      "settings": {
        "foreground": "#c0caf5"
      }
    },
    {
      "name": "Markdown Inline Code",
      "scope": [
        "markup.inline.raw",
        "markup.raw.inline"
      ],
      "settings": {
        "foreground": "#9ece6a",
        "background": "#1a1d2433"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#7aa2f7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#bb9af7",
        "fontStyle": "italic"
      }
    }
  ]
}