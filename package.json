{
  "name": "aether-dark",
  "publisher": "diogo-aether",
  "displayName": "Aether Themes",
  "description": "Coleção premium com temas modernos para Visual Studio Code. Legibilidade perfeita, sintaxe refinada e integração total com diversas linguagens de programação e muito mais.",
  "version": "1.3.3",
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#121212",
    "theme": "dark"
  },
  "previewImages": [
    "images/aether-themes.gif"
  ],
  "keywords": [
    "theme",
    "vscode",
    "dark theme",
    "typescript",
    "developer",
    "modern",
    "clean",
    "purple",
    "neon",
    "aether",
    "productivity",
    "minimalist",
    "syntax-highlighting",
    "dark-mode"
  ],
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Diiipereira/aether-theme.git"
  },
  "bugs": {
    "url": "https://github.com/Diiipereira/aether-theme/issues"
  },
  "homepage": "https://github.com/Diiipereira/aether-theme/blob/main/README.md",
  "engines": {
    "vscode": "^1.70.0"
  },
  "scripts": {
    "build": "ts-node src/index.ts",
    "package": "npm run build && vsce package",
    "publish": "npm run build && vsce publish",
    "watch": "ts-node-dev --respawn src/index.ts",
    "test": "ts-node src/test/test-theme.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vscode/vsce": "^2.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "contributes": {
    "themes": [
      {
        "label": "Coffee",
        "uiTheme": "vs-dark",
        "path": "./themes/aether-coffee.json"
      },
      {
        "label": "Coffee Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/aether-coffee-dark.json"
      },
      {
        "label": "Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/aether-dark.json"
      },
      {
        "label": "Dark Space",
        "uiTheme": "vs-dark",
        "path": "./themes/aether-dark-space.json"
      },
      {
        "label": "Emerald",
        "uiTheme": "vs-dark",
        "path": "./themes/aether-emerald.json"
      },
      {
        "label": "Light",
        "uiTheme": "vs",
        "path": "./themes/aether-light.json"
      }
    ]
  },
  "dependencies": {
    "polished": "^4.3.1"
  }
}
