  "version": "1.2.3",
  "icon": "logo.png",

  "galleryBanner": {
    "color": "#121212",
    "theme": "dark"
  },

  "previewImages": [
    "images/aether-dark.png",
    "images/aether-darkspace.png",
    "images/aether-coffe.png",
    "images/aether-light.png"
  ],

  "keywords": [
    "theme",
    "themes",
    "vscode",
    "vscode theme",
    "dark theme",
    "light theme",
    "typescript",
    "developer",
    "modern",
    "clean",
    "purple",
    "blue",
    "coffee",
    "aesthetic",
    "ui",
    "color",
    "aether"
  ],

  "categories": ["Themes"],
  "repository": {
    "type": "git",
    "url": "https://github.com/Diiipereira/aether-theme.git"
  },

  "engines": {
    "vscode": "^1.70.0"
  },

  "contributes": {
    "themes": [
      {
        "label": "Aether Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/aether-dark.json"
      },
      {
        "label": "Aether Dark Space",
        "uiTheme": "vs-dark",
        "path": "./themes/aether-darkspace.json"
      },
      {
        "label": "Aether Coffe",
        "uiTheme": "vs-dark",
        "path": "./themes/aether-coffe.json"
      },
      {
        "label": "Aether Light",
        "uiTheme": "vs",
        "path": "./themes/aether-light.json"
      }
    ]
